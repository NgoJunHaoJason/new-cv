{
  "name": "my-cv",
  "version": "0.0.0",
  "description": "Jason's CV",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b main -d dist",
